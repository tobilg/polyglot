{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Polyglot SQL API Documentation",
  "entryPoints": [
    "../sdk/src/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "tsconfig": "../sdk/tsconfig.typedoc.json",
  "router": "structure-dir",
  "out": "dist",
  "readme": "../sdk/README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "titleLink": "/",
  "navigationLinks": {
    "GitHub": "https://github.com/tobilg/polyglot",
    "npm": "https://www.npmjs.com/package/@polyglot-sql/sdk"
  },
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Query Builders",
    "Expression Helpers",
    "Aggregates",
    "String Functions",
    "Math Functions",
    "Date/Time Functions",
    "Null Handling",
    "Window Functions",
    "AST",
    "Validation",
    "Types",
    "*"
  ]
}
