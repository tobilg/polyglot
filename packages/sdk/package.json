{
  "name": "@polyglot-sql/sdk",
  "version": "0.1.0",
  "description": "SQL dialect translator powered by WebAssembly",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/cdn/polyglot.esm.js",
  "jsdelivr": "./dist/cdn/polyglot.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cdn": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cdn/polyglot.esm.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:bindings": "cd ../.. && make copy-bindings",
    "build:wasm": "cd ../../crates/polyglot-sql-wasm && ~/.cargo/bin/wasm-pack build --target bundler --release --out-dir ../../packages/sdk/wasm",
    "build:esm": "vite build",
    "build:umd": "vite build --config vite.config.umd.ts",
    "build": "npm run build:esm && npm run build:umd",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sql",
    "transpile",
    "dialect",
    "wasm",
    "webassembly",
    "postgresql",
    "mysql",
    "bigquery",
    "snowflake",
    "duckdb"
  ],
  "author": "polyglot contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tobilg/polyglot"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.7.0",
    "vite": "7.3.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^4.0.0"
  }
}
